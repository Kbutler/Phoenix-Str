<h1>Log in</h1>

<%= form_tag "http://localhost:3000/admin_session#create" do %>

  <p>
    <%= label_tag :name %><br />
    <%= text_field_tag :name, params[:name] %>
  </p>
  <p>
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
    </p>
    <p class="button"><%= submit_tag %></p>

    <% end %>
